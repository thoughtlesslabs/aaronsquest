	init_player(3,1,31,0)
	init_map()
	add_starts()
	add_keycode()
	add_movables()
	reading=false
	show_inventory=false
	gameover = false
	debug = ""
	debug2= ""
end

function init_map()
 -- animation timers
 timer = 0
